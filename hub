   for i,v in pairs(game.ReplicatedStorage.Player_Data[game.Players.LocalPlayer.Name].EquippedItemStats:GetChildren()) do
	if v:IsA("StringValue") then
	v.Value = "Banigaru Mask"
	end
	end
   end,
